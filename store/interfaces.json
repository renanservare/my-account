{
  "my-account": {
    "component": "index",
    "required": [
      "my-account-pages",
      "my-account-defaultPage",
      "my-account-menu"
    ],
    "allowed": [
      "profile-display",
      "profile-input"
    ]
  },
  "my-account-defaultPage": {
    "component": "DefaultPage"
  },
  "my-account-pages": {
    "component": "vtex.render-runtime/ExtensionContainer",
    "allowed": [
      "my-orders",
      "my-cards",
      "my-account-page"
    ]
  },
  "my-account-page": {
    "component": "*"
  },
  "my-account-menu": {
    "component": "vtex.render-runtime/LayoutContainer",
    "allowed": [
      "my-profile-link",
      "my-addresses-link",
      "my-orders-link",
      "my-cards-link",
      "my-account-link"
    ]
  },
  "my-account-link": {
    "component": "*"
  },
  "my-addresses-link": {
    "component": "MyAddressesLink"
  },
  "my-profile-link": {
    "component": "MyProfileLink"
  },
  "profile-display": {
    "component": "vtex.render-runtime/ExtensionContainer"
  },
  "profile-input": {
    "component": "vtex.render-runtime/ExtensionContainer"
  }
}
